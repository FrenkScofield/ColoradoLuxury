                        <div class="input-group specialSpace col-6">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="inputGroup-sizing-default">Publishable API KEY</span>
                            </div>
                            <input type="text" class="form-control" aria-label="MinumumDuration" aria-describedby="inputGroup-sizing-default" >
                        </div>
                    </div>
                </div>
                <button type="button" class="btn btn-outline-success float-right">Success</button>
            </div>
        </div>
    </div>
</div>

